{
    "name": "bhaveshdaswani/laravel-batch",
    "description": "Insert and update batch (bulk) in laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bhavesh Daswani",
            "email": "bhaveshdaswani93@gmail.com"
        }
    ],
    "require": {
        "phpunit/phpunit": "^9.3@dev"
    },
    "autoload": {
        "psr-4": {
            "BhaveshDaswani\\Batch\\": "src/"
        },
        "files": [
            "src/Common/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "BhaveshDaswani\\Batch\\BatchServiceProvider"
            ],
            "aliases": {
                "Batch": "BhaveshDaswani\\Batch\\BatchFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
